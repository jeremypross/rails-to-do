  <body>
    <h1>TO DO APP</h1>
    <h3>Create Account:</h3>

    <%= form_for User.new do |f| %>
    <%= f.label :first_name %>
    <br />
    <%= f.text_field :first_name %>
    <br />
    <%= f.label :last_name %>
    <br />
    <%= f.text_field :last_name %>
    <br />
    <%= f.label :email %>
    <br />
    <%= f.text_field :email %>
    <br />
    <%= f.label :password %>
    <br />
    <%= f.text_field :password %>
    <%= f.submit %>
    <% end %>


